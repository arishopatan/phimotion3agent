{
  "name": "phimotion3agent",
  "version": "1.0.0",
  "description": "Professional Motion Analysis Dashboard - Advanced motion tracking, joint angle analysis, and symmetry assessment with beautiful visualizations",
  "private": true,
  "author": "PhiMotion3Agent Team",
  "license": "MIT",
  "homepage": "https://phimotion3agent.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/phimotion3agent.git"
  },
  "keywords": [
    "motion-analysis",
    "biomechanics",
    "sports-analysis",
    "rehabilitation",
    "joint-tracking",
    "nextjs",
    "typescript",
    "tailwindcss",
    "recharts",
    "dashboard"
  ],
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:clean": "rm -rf .next && npm run dev",
    "dev:safe": "rm -rf .next && rm -rf node_modules/.cache && npm run build && npm run dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "rm -rf .next out dist",
    "analyze": "ANALYZE=true npm run build"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.539.0",
    "next": "15.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "recharts": "^3.1.2",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.6",
    "typescript": "^5"
  }
}
